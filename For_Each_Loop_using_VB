Imports System
Module Program
    Sub Main()
        Dim size As Integer
        Console.WriteLine("Enter Size of Array : ")
        size = Console.ReadLine()
        Dim Arr(size) As Integer
        Dim ArrayItem As Integer
        Dim i As Integer
        For i = 0 To size
            Arr(i) = Console.ReadLine()
        Next

        For Each ArrayItem In Arr
            Console.WriteLine(ArrayItem)
        Next
    End Sub
End Module